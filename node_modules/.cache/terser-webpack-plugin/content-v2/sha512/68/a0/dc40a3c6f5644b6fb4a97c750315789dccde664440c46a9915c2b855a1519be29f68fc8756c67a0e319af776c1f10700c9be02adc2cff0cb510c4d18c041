-lazy-load-image-component/src/effects/blur.css'\nimport { BrowserRouter as Router } from 'react-router-dom'\n\nimport './_base.scss'\nimport store from './redux/store'\n\nReactDOM.render(\n   <Provider store={store}>\n      <Router>\n         <App />\n      </Router>\n   </Provider>,\n\n   document.getElementById('root')\n)\n","// extracted by mini-css-extract-plugin","// extracted by mini-css-extract-plugin","// extracted by mini-css-extract-plugin","// extracted by mini-css-extract-plugin"]}}